{
	"name": "hypixel.ts",
	"version": "2.0.0",
	"description": "A simple, easy to use wrapper for Hypixel API.",
	"homepage": "https://hypixel.js.org/",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"author": "FC5570",
	"license": "MIT",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		}
	},
	"scripts": {
		"gen-esm": "yarn gen-esm-wrapper dist/index.js dist/index.mjs",
		"build": "yarn tsc -b src",
		"lint": "yarn eslint src --fix --ext .ts",
		"format": "yarn prettier --write src",
		"test": "yarn vitest",
		"coverage": "yarn test --coverage",
		"prepare": "yarn husky install .github/.husky",
		"prepublishOnly": "yarn build && yarn gen-esm",
		"bump": "yarn cliff-jumper",
		"release": "git push && git push --tags && npm publish"
	},
	"dependencies": {
		"@sapphire/async-queue": "^1.5.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.4.1",
		"@commitlint/config-conventional": "^18.4.0",
		"@favware/cliff-jumper": "^2.2.1",
		"@sapphire/eslint-config": "^5.0.2",
		"@sapphire/prettier-config": "^2.0.0",
		"@sapphire/ts-config": "^5.0.0",
		"@types/node": "^20.0.0",
		"@typescript-eslint/eslint-plugin": "^6.10.0",
		"@typescript-eslint/parser": "^6.10.0",
		"@vitest/coverage-c8": "^0.33.0",
		"@vitest/coverage-v8": "^1.0.0",
		"eslint": "^8.53.0",
		"eslint-plugin-prettier": "^5.0.1",
		"gen-esm-wrapper": "^1.1.3",
		"git-cliff": "^1.4.0",
		"husky": "^8.0.3",
		"lint-staged": "^15.1.0",
		"prettier": "^3.1.0",
		"typedoc": "^0.25.3",
		"typescript": "^5.2.2",
		"vitest": "^1.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FC5570/hypixel.ts"
	},
	"bugs": {
		"url": "https://github.com/FC5570/hypixel.ts/issues"
	},
	"keywords": [
		"hypixel",
		"hypixel-api",
		"hypixel-api-wrapper",
		"hypixel.js",
		"hypixel.ts"
	],
	"files": [
		"dist/",
		"*.md",
		"!README.md"
	],
	"lint-staged": {
		"*.{ts,js}": "yarn lint"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"packageManager": "yarn@4.1.0",
	"prettier": "@sapphire/prettier-config"
}
