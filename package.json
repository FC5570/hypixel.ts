{
	"name": "hypixel.ts",
	"version": "1.0.6",
	"description": "A simple, easy to use wrapper for Hypixel API.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"author": "FC5570",
	"license": "MIT",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		}
	},
	"scripts": {
		"gen-esm": "yarn gen-esm-wrapper dist/index.js dist/index.mjs",
		"build": "yarn tsc -b src",
		"lint": "yarn eslint --fix src --ext .ts",
		"format": "yarn prettier --write .",
		"test": "yarn vitest",
		"prepare": "yarn husky install",
		"prepublish": "yarn build && yarn gen-esm",
		"update": "yarn upgrade-interactive --latest"
	},
	"dependencies": {
		"@discordjs/collection": "^0.4.0",
		"@sapphire/async-queue": "^1.1.9",
		"petitio": "^1.4.0",
		"tslib": "^2.3.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.0.2",
		"@commitlint/config-conventional": "^16.0.0",
		"@sapphire/eslint-config": "^4.0.9",
		"@sapphire/prettier-config": "^1.2.8",
		"@sapphire/ts-config": "^3.1.7",
		"@types/eslint": "^8",
		"@types/node": "^17.0.8",
		"@types/prettier": "^2",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.6.0",
		"gen-esm-wrapper": "^1.1.3",
		"husky": "^7.0.4",
		"lint-staged": "^12.1.7",
		"prettier": "^2.5.1",
		"typescript": "^4.5.4",
		"vitest": "^0.0.141"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FC5570/hypixel.ts"
	},
	"bugs": {
		"url": "https://github.com/FC5570/hypixel.ts/issues"
	},
	"keywords": [
		"hypixel",
		"hypixel-api",
		"hypixel-api-wrapper",
		"hypixel.js",
		"hypixel.ts"
	],
	"packageManager": "yarn@3.1.1",
	"prettier": "@sapphire/prettier-config"
}
