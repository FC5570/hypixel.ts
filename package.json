{
	"name": "hypixel.ts",
	"version": "1.1.1",
	"description": "A simple, easy to use wrapper for Hypixel API.",
	"homepage": "https://hypixel.js.org/",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"author": "FC5570",
	"license": "MIT",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		}
	},
	"scripts": {
		"gen-esm": "yarn gen-esm-wrapper dist/index.js dist/index.mjs",
		"build": "yarn tsc -b src",
		"lint": "yarn eslint src --fix --ext .ts",
		"format": "yarn prettier --write src",
		"test": "yarn vitest",
		"coverage": "yarn vitest --coverage",
		"prepare": "yarn husky install",
		"prepublishOnly": "yarn build && yarn gen-esm",
		"update": "yarn upgrade-interactive --latest"
	},
	"dependencies": {
		"@discordjs/collection": "^1.1.0",
		"@sapphire/async-queue": "^1.5.0",
		"petitio": "^1.4.0",
		"tslib": "^2.4.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@sapphire/eslint-config": "^4.3.8",
		"@sapphire/prettier-config": "^1.4.4",
		"@sapphire/ts-config": "^3.3.4",
		"@types/eslint": "^8.4.6",
		"@types/node": "^18.7.23",
		"@types/prettier": "^2.7.1",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.38.1",
		"@typescript-eslint/parser": "^5.38.1",
		"@vitest/coverage-c8": "^0.23.4",
		"c8": "^7.12.0",
		"eslint": "^8.24.0",
		"gen-esm-wrapper": "^1.1.3",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"typedoc": "^0.23.15",
		"typescript": "^4.8.4",
		"uuid": "^9.0.0",
		"vitest": "^0.23.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FC5570/hypixel.ts"
	},
	"bugs": {
		"url": "https://github.com/FC5570/hypixel.ts/issues"
	},
	"keywords": [
		"hypixel",
		"hypixel-api",
		"hypixel-api-wrapper",
		"hypixel.js",
		"hypixel.ts"
	],
	"files": [
		"dist/",
		"*.md",
		"!README.md"
	],
	"lint-staged": {
		"*.{ts,js}": "yarn lint"
	},
	"packageManager": "yarn@3.1.1",
	"prettier": "@sapphire/prettier-config"
}
